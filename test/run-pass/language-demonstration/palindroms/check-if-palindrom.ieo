# - run:
#     stdin:
#       - "aabbcdedcbbaa"
#     stdout: 
#       - "yes"

fun main() {
    let a = readln();
    let obverse = a;
    let reverse = "";
    while (length(a) > 0) {
        stradd(reverse, strcut(length(a)-1, length(a), a));
    }
    if (obverse == reverse) { 
        println("yes");
    } else {
        println("no");
    }
}
