#     stdin:
#       - "1621"
#     stdout: 
#       - "yes"

fun main() {
    let a = int(readln());
    if (a <= 0) { println("The number should be positive"); return fail; }
    let factor = 2;
    while (a >= factor * factor) {
        let tmp = a / factor;
        if (tmp * factor == a) {
            println("no");
            return fail;
        }
        factor = factor + 1;
    }
    println("yes");
}
