# - run:
#     stdin:
#       - "1621"
#     stdout:
#       - "yes"

fun main() {
    a = int(scanln());
    if (check_if_prime_number(a)) {
      println("yes");
    } else {
      println("no");
    }
}

fun check_if_prime_number(a) {
    if(a <= 0) {
        println("The number should be positive");
        return fail none;
    }
    factor = 2;
    while(a >= factor * factor) {
        tmp = a / factor;
        if (tmp * factor == a) {
            return fail none;
        }
        factor = factor + 1;
    }
}
