# - run:
#     stdin:
#       - "5"
#     stdout:
#       - "120"

fun main() {
  n = int(scanln());
  println(factorial(n));
}

fun factorial(n) {
  return if (n < 0) {
    println("The number should be non-negative");
    fail none
  } else if (n < 2) {
    1
  } else {
    n * factorial(n - 1)
  }
}
