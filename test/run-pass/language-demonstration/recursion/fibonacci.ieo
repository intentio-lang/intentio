# - run:
#     stdin:
#       - "8"
#     stdout: 
#       - "21"

fun main() {
    let n = int(readln());
    fibonacci(n);
}

fun fibonacci(n) {
    if(n < 0) { println("The number should be non-negative"); return fail; }
    if(n == 0) { return 0; }
    if(n == 1) { return 1; }
    return main(n - 1) + main(n - 2);
}
