import check_classic_notation:check_classic_notation
import classic_notation_to_rnp:rnp

fun main() {
  while (input = scanln()) {
    if (check_classic_notation(input)) {
      println(rnp(input));
    } else {
      println("Incorrect input; please use only proper expressions in classic notation build with: (,),*,+,a,b,c");
    }
  }
}
