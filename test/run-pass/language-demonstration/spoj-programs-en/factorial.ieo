# https://www.spoj.com/problems/FCTRL/

fun main() {
    n = int(scanln());
    i = 0;
    while(i < n) {
        factorial(n);
        i = i + 1;
    }
}

fun factorial(n) {
    a = int(scanln());
    div = 5;
    res = 0;
    while(n / div > 0) {
        res = res + n / div;
        div = div * 5;
    }
    println(res);
}
